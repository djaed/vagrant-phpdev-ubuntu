{
  "server": "phpdev",
  "ufw": {
    "allows": ["http", "https", "22"]
  },
  "ssh": {
    "port": "22"
  },
  "git": {
    "user": {
      "email": "vagrant@example.com",
      "name": "vagrant"
    }
  },
  "php": {
    "memory_limit": "512M",
    "display_errors": "On",
    "zend_extensions": ["/usr/lib/php5/20121212/xdebug.so"],
    "extensions": ["xhprof.so", "mongo.so", "zmq.so", "gearman.so"],
    "date": {
      "timezone": "Asia/Tokyo"
    }
  },
  "apache2": {
    "var/www": {
      "allow_override": "All"
    }
  },
  "mysql": {
    "root": {
      "password": "root"
    }
  }
}
